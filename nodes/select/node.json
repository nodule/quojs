{
  "name": "select",
  "ns": "quojs",
  "description": "QuoJS selector",
  "require": {
    "quoque": "psichi/quoque"
  },
  "expose": ["window", "document"],
  "phrases": {
    "active": "Selecting {input.selector}"
  },
  "ports": {
    "input": {
      "selector": {
        "title": "Selector",
        "type": "any",
        "required": true
      }
    },
    "output": {
      "out": {
        "title": "Selection",
        "type": "object"
      }
    }
  }
}
