      "name": "addClass",
      "ns": "quojs",
      "description": "QuoJS addClass",
      "phrases": {
        "active": "Adding class {input.className}"
      },
      "ports": {
        "input": {
          "selection": {
            "title": "Selection",
            "type": "any",
            "required": true
          },
          "className": {
            "title": "Class Name",
            "type": "string",
            "required": true
          }
        },
        "output": {}
      },
      "fn": "output = function() {\n  $.selection.addClass($.className)\n  done()\n}\n"
    },
